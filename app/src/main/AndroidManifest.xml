<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wecent.smartdialog.sample">

    <application
        android:name="com.wecent.sample.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wecent.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="smart_dialog_design_width"
            android:value="1080" />
        <meta-data
            android:name="smart_dialog_design_height"
            android:value="1920" />

        <activity android:name="com.wecent.sample.list.ListViewActivity"/>
    </application>

</manifest>